{
  "name": "@sigloop/x402",
  "version": "0.0.1",
  "description": "x402 HTTP payment middleware for AI agents",
  "author": "in-th3-l00p",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./advanced": {
      "import": "./dist/advanced.js",
      "types": "./dist/advanced.d.ts"
    },
    "./constants": {
      "import": "./dist/constants.js",
      "types": "./dist/constants.d.ts"
    }
  },
  "files": [

{
  "name": "@sigloop/defi",
  "version": "0.0.1",
  "description": "DeFi primitive wrappers for ERC-7579 executor modules",
  "author": "in-th3-l00p",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./advanced": {
      "import": "./dist/advanced.js",
      "types": "./dist/advanced.d.ts"
    },
    "./constants": {
      "import": "./dist/constants.js",
      "types": "./dist/constants.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "example:swap": "tsx examples/swap.ts",
    "example:lending": "tsx examples/lending.ts",
    "example:composable": "tsx examples/composable.ts"
  },
  "dependencies": {
    "viem": "^2.28.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "keywords": [
    "defi",
    "swap",
    "lending",
    "staking",
    "erc-7579"
  ]
}
